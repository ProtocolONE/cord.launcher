{
  "name": "cord.web-electron",
  "version": "0.1.0",
  "description": "Cord's web and electron app",
  "productName": "cord.web-electron",
  "author": "borisbutenko <iamborisbutenko@gmail.com>",
  "private": true,
  "scripts": {
    "serve": "quasar dev -m ssr -t mat",
    "electron": "quasar dev -m electron -t mat",
    "build": "quasar build -t mat",
    "build:ssr": "quasar build -m ssr -t mat",
    "build:electron": "quasar build -m electron -t mat",
    "build:electron:linux": "quasar build -m electron linux -t mat -b builder",
    "build:electron:win32": "quasar build -m electron win32 -t mat -b builder",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix"
  },
  "dependencies": {
    "@vue/eslint-config-standard": "^4.0.0",
    "axios": "^0.18.0",
    "lodash-es": "^4.17.11",
    "moment": "^2.24.0",
    "raven-js": "^3.27.0",
    "vue-i18n": "^7.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "devtron": "1.4.0",
    "electron": "3.0.8",
    "electron-debug": "2.0.0",
    "electron-devtools-installer": "2.2.4",
    "electron-packager": "^12.0.1",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "quasar-cli": "^0.17.0",
    "strip-ansi": "=3.0.1"
  },
  "engines": {
    "node": ">= 11.10.0",
    "npm": ">= 6.7.0",
    "yarn": ">= 1.13.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "gitHooks": {
    "pre-commit": "ESLINT_STRICT=true lint-staged",
    "pre-push": "set \"NODE_PATH=./source\" mocha './source/**/*.spec.js' --require babel-register"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.ts": [
      "eslint"
    ],
    "*.vue": [
      "eslint"
    ]
  },
  "resolutions": {
    "ajv": "6.8.1"
  }
}
