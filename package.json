{
  "name": "cord.launcher",
  "version": "0.0.3-beta",
  "description": "Cord launcher based on Electron and Wep Application",
  "productName": "qilincord",
  "author": "borisbutenko <iamborisbutenko@gmail.com>",
  "license": "MIT",
  "main": "./src-electron/main-process/electron-main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProtocolONE/cord.launcher.git"
  },
  "scripts": {
    "dev": "quasar dev -t mat",
    "dev:ssr": "MODE=ssr quasar dev -m ssr -t mat",
    "dev:electron": "MODE=electron quasar dev -m electron -t mat",
    "build": "yarn build:ssr && yarn build:electron:all",
    "build:ssr": "MODE=ssr quasar build -m ssr -t mat",
    "build:electron": "MODE=electron quasar build -m electron --target all -t mat",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "electron-log": "^3.0.1",
    "electron-packager": "^13.1.1",
    "electron-updater": "^4.0.6",
    "lodash-es": "^4.17.11",
    "vue-i18n": "^7.3.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/lodash-es": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "devtron": "1.4.0",
    "electron": "4.0.5",
    "electron-builder": "^20.39.0",
    "electron-debug": "2.1.0",
    "electron-devtools-installer": "2.2.4",
    "eslint": "^5.15.3",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "moment": "^2.24.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "quasar-cli": "^0.17.0",
    "strip-ansi": "=3.0.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333",
    "vue-class-component": "^7.0.1",
    "vue-property-decorator": "^8.0.0",
    "vuex-class": "^0.3.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "resolutions": {
    "ajv": "6.8.1"
  }
}
